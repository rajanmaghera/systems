{
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "breadcrumbs.enabled": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "Cousine Nerd Font Mono, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.lineHeight": 1.6,
    "editor.minimap.autohide": true,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.size": "fit",
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "trailing",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "editor.smoothScrolling": true,
    "editor.unicodeHighlight.nonBasicASCII": true,
    "explorer.decorations.colors": false,
    "files.exclude": {
        "**/.direnv": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "github.copilot.editor.enableAutoCompletions": true,
    "scm.diffDecorationsGutterVisibility": "hover",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "none",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.lineHeight": 1.4,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.tabs.defaultIcon": "symbol-array",
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.tabs.showActions": "never",
    "terminal.integrated.tabs.showActiveTerminal": "never",
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false,
        "<C-t>": false,
        "<C-p>": false
    },
    "window.autoDetectColorScheme": true,
    "window.commandCenter": false,
    "workbench.activityBar.location": "bottom",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.showTabs": "none",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-auto",
    "workbench.preferredDarkColorTheme": "Fleet Grey",
    "workbench.preferredLightColorTheme": "JetBrains Rider New UI Lighter",
    "workbench.startupEditor": "none",
    "window.title": "${rootNameShort}${separator}${activeEditorMedium}",
    "window.titleSeparator": "/",
    "terminal.integrated.enableImages": true,
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.shellIntegration.enabled": false,
    "workbench.colorCustomizations": {
        "[Fleet Grey]": {
            "activityBarBadge.background": "#303030",
            "badge.background": "#303030",
            "icon.foreground": "#5a5a5a",
            "editorCodeLens.foreground": "#616161"
        }
    },
    "github.copilot.editor.enableCodeActions": false,
    "editor.lightbulb.enabled": "off",
    "redhat.telemetry.enabled": false,
    "workbench.statusBar.visible": false
}
